{
    "builds": [
        {
            "src": "vite.config.ts",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/pwa/(.*)",
            "dest": "/pwa/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}